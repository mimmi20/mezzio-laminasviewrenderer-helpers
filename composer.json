{
  "name": "mimmi20/mezzio-laminasviewrenderer-helpers",
  "type": "library",
  "description": "Provides a GitHub repository mezzio-laminasviewrenderer-helpers for a PHP library, using GitHub actions.",
  "homepage": "https://github.com/mimmi20/mezzio-laminasviewrenderer-helpers",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4.0 || ^8.0.0",
    "ext-mbstring": "*",
    "laminas/laminas-json": "^3.2.0",
    "mezzio/mezzio-laminasviewrenderer": "^2.2.0"
  },
  "conflict": {
    "container-interop/container-interop": "<1.2.0"
  },
  "require-dev": {
    "infection/infection": "^0.21.5",
    "mimmi20/coding-standard": "^2.1.1",
    "pepakriz/phpstan-exception-rules": "^0.11.6",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^0.12.83",
    "phpstan/phpstan-deprecation-rules": "^0.12.6",
    "phpstan/phpstan-phpunit": "~0.12.18",
    "phpunit/phpunit": "^9.5.4"
  },
  "suggest": {
    "ext-dom": "required to use the Sitemap ViewHelper",
    "ext-intl": "required to use the Translator",
    "ext-libxml": "required to use the Sitemap ViewHelper",
    "laminas/laminas-config": "^3.5.0, to provide page configuration (optional, as arrays and Traversables are also allowed)",
    "laminas/laminas-i18n": "^2.11.0, required to use the Translator",
    "laminas/laminas-servicemanager": "^3.6.4, to use the navigation factories",
    "laminas/laminas-uri": "^2.7.1 || ^2.8.0, required to use the Sitemap ViewHelper",
    "laminas/laminas-validator": "^2.13.5 || ^2.14.0, required to use the Sitemap ViewHelper",
    "mimmi20/mezzio-generic-authorization": "^1.0.1, to use authrization roles or privileges"
  },
  "config": {
    "platform": {
      "php": "7.4.0"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "config-provider": "Mezzio\\LaminasView\\Helper\\ConfigProvider"
    }
  },
  "autoload": {
    "psr-4": {
      "Mezzio\\LaminasView\\Helper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MezzioTest\\LaminasView\\Helper\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/mimmi20/mezzio-laminasviewrenderer-helpers/issues",
    "source": "https://github.com/mimmi20/mezzio-laminasviewrenderer-helpers"
  }
}
